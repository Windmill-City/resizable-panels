{
  "name": "@local/resizable-panels",
  "version": "1.0.0",
  "description": "Headless Resizable Panels",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"pnpm watch\" \"pnpm -r dev\"",
    "watch": "tsup --watch",
    "postinstall": "pnpm build",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "concurrently": "^9.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "react",
    "resizable",
    "panels",
    "headless",
    "typescript"
  ],
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.0.0"
}
